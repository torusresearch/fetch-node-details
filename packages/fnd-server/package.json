    "@types/express": "^4.17.14",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/morgan": "^1.9.3",
    "assert": "^2.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
