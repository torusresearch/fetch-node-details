{
  "name": "@toruslabs/fnd-server",
  "version": "11.0.0",
  "description": "",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "serve": "nodemon --config nodemon.json src/index.ts",
    "prod": "cross-env NODE_ENV=production && node dist/src/index.js",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:server",
    "build:server": "tsc",
    "lint": "eslint --fix 'src/**/*.ts'",
    "pre-commit": "lint-staged --cwd .",
    "dev": "docker-compose -f ../../docker-compose.dev.yml up --build",
    "down": "docker-compose -f ../../docker-compose.dev.yml down"
  },
  "dependencies": {
    "@sentry/node": "^7.56.0",
    "@sentry/tracing": "^7.56.0",
    "@toruslabs/constants": "^11.0.0",
    "@toruslabs/fnd-base": "^11.0.0",
    "@toruslabs/http-helpers": "^4.0.0",
    "@toruslabs/loglevel-sentry": "^5.0.0",
    "celebrate": "^15.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.6.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "json-stable-stringify": "^1.0.2",
    "loglevel": "^1.8.1",
    "morgan": "^1.10.0",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@toruslabs/eslint-config-node": "^2.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/morgan": "^1.9.4",
    "assert": "^2.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "wait-port": "^1.0.4"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,ts}": "eslint --cache --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
