{
  "name": "fnd-server",
  "version": "8.1.1",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "servers": "node scripts/runServers.js",
    "depcheck": "depcheck .",
    "serve": "nodemon --config nodemon.json src/index.ts",
