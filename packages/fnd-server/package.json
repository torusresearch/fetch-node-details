    "atob": "^2.1.2",
    "axios": "^0.27.2",
    "celebrate": "^15.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "loglevel": "^1.8.0",
    "morgan": "^1.10.0",
    "redis": "^4.3.1",
